#include<stdio.h>
int fib(int n)
{
    // printf("Number is %d\n",n);
    if (n==0)
    {
        return 0;
    }
    if (n==1||n==2)
    {
        return 1;
    }
    else
    {
        return (fib(n-1)+fib(n-2));
    }
}
int main()
{
    int a,ans;
    printf("Enter number:");
    scanf("%d",&a);
    ans = fib(a);
    printf("Answer is %d",ans);
    return 0;
}
